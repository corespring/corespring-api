    <responseProcessing type="script">
        <script type="text/javascript">

            <![CDATA[
            var choose = function (elems, len) {
              var result = [];
              for (var i=0; i < elems.length; i++) {
                if (len == 1) {
                  result.push([elems[i]]);
                } else {
                  var remainingItems = choose(elems.slice(i+1, elems.length), len - 1);
                  for (var j=0; j < remainingItems.length; j++)
                    result.push([elems[i]].concat(remainingItems[j]));
                }
              }
              return result;
            };

            var getScore = function (answer, choices) {
              var score = 0;

              for (var i=0;i < choices.length;i++) {
                if(answer.indexOf(choices[i][0]) <  answer.indexOf(choices[i][1])) {
                  score +=1;
                }
              }
              return score;
            };

            // get sequence
            var choices = choose(["2","7","18","21","28","35"], 2);

                var answerAsArray = [Q_01.value, Q_02.value,Q_03.value, Q_04.value, Q_05.value, Q_06.value];

                var points = getScore(answerAsArray, choices);
                var maxPoints = 15;
                var score = (1 / maxPoints) * points;
                var outcome = {};
                outcome.score = score;
                outcome;

            ]]>



        </script>
