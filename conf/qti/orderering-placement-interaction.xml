                        <semantics>
                            <mrow>
                                <mfrac>
                                    <mrow>
                                        <mn>2</mn>
                                    </mrow>
                                    <mrow>
                                        <mn>3</mn>
                                    </mrow>
                                </mfrac>
                                <mrow>
                                    <mrow>
                                        <mo stretchy="false">(</mo>
                                        <mrow>
                                            <mrow>
                                                <mrow>
                                                    <mn>2</mn>
                                                    <mrow>
                                                        <mi>x</mi>
                                                    </mrow>
                                                </mrow>
                                                <mo stretchy="false">âˆ’</mo>
                                                <mn>1</mn>
                                            </mrow>
                                        </mrow>
                                        <mo stretchy="false">)</mo>
                                    </mrow>
                                </mrow>
                            </mrow>
                            <annotation encoding="StarMath 5.0">{2} over {3} ({2{x}} - 1) + 2{1} over {3} = 7 + {1} over {2}{x}</annotation>
                        </semantics>
                    </math></simpleChoice>
            <simpleChoice identifier="d">1</simpleChoice>
