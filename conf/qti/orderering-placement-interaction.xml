            <simpleChoice identifier="f">F</simpleChoice>
            <simpleChoice identifier="g">G</simpleChoice>
            <simpleChoice identifier="h" fixed="true">H</simpleChoice>
        </orderInteraction>
    </itemBody>
</assessmentItem>

