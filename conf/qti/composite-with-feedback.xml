        <choiceInteraction responseIdentifier="mexicanPresident" shuffle="false" maxChoices="1">
            <prompt>Who is the President of Mexico?</prompt>
            <simpleChoice identifier="bush"> George W Bush
                <!-- in feedback directive we could infer that if the variable in outcomeIdentifier is an array we should do a contains() instead of '=='-->
                <feedbackInline csFeedbackId="bush" outcomeIdentifier="responses.mexicanPresident.value" identifier="bush" showHide="show"></feedbackInline>
            </simpleChoice>
            <simpleChoice identifier="blair"> Tony Blair
                <feedbackInline csFeedbackId="blair" outcomeIdentifier="responses.mexicanPresident.value" identifier="blair" showHide="show"></feedbackInline>
            </simpleChoice>
            <simpleChoice identifier="calderon"> Felipe Calderon
                <feedbackInline csFeedbackId="calderon" outcomeIdentifier="responses.mexicanPresident.value" identifier="calderon" showHide="show" ></feedbackInline>
            </simpleChoice>
            <simpleChoice identifier="sharon"> Ariel Sharon
                <feedbackInline csFeedbackId="sharon" outcomeIdentifier="responses.mexicanPresident.value" identifier="sharon" showHide="show"></feedbackInline>
            </simpleChoice>
        </choiceInteraction>

        <choiceInteraction responseIdentifier="irishPresident" shuffle="false" maxChoices="1">
        <prompt>Who is the President of Ireland?</prompt>
        <simpleChoice identifier="robinson"> Mary Robinson <feedbackInline
                csFeedbackId="robinson" outcomeIdentifier="responses.irishPresident.value" identifier="robinson" showHide="show"></feedbackInline>
        </simpleChoice>
        <simpleChoice identifier="higgins"> Michel D. Higgins <feedbackInline
                csFeedbackId="higgins" outcomeIdentifier="responses.irishPresident.value" identifier="higgins" showHide="show"></feedbackInline>
        </simpleChoice>
        <simpleChoice identifier="guinness"> Arthur Guinness <feedbackInline
                csFeedbackId="guinness" outcomeIdentifier="responses.irishPresident.value" identifier="guinness" showHide="show"
                ></feedbackInline>
        </simpleChoice>
        <simpleChoice identifier="adams"> Gerry Adams <feedbackInline
                csFeedbackId="adams" outcomeIdentifier="responses.irishPresident.value" identifier="adams" showHide="show"></feedbackInline>
        </simpleChoice>
        </choiceInteraction>



        <choiceInteraction responseIdentifier="rainbowColors" shuffle="false" maxChoices="0">
        <prompt>Which colors are in a rainbow?</prompt>
        <simpleChoice identifier="blue"> Blue <feedbackInline
                csFeedbackId="blue" outcomeIdentifier="responses.rainbowColors.value" identifier="blue" showHide="show"></feedbackInline>
        </simpleChoice>
        <simpleChoice identifier="violet"> Violet <feedbackInline
                csFeedbackId="violet" outcomeIdentifier="responses.rainbowColors.value" identifier="violet" showHide="show"></feedbackInline>
        </simpleChoice>
        <simpleChoice identifier="white"> White <feedbackInline
                csFeedbackId="white" outcomeIdentifier="responses.rainbowColors.value" identifier="white" showHide="show"
                ></feedbackInline>
        </simpleChoice>
        <simpleChoice identifier="red"> Red <feedbackInline
                csFeedbackId="red" outcomeIdentifier="responses.rainbowColors.value" identifier="red" showHide="show"></feedbackInline>
        </simpleChoice>
        </choiceInteraction>



        <prompt>Identify the missing word in this famous quote from Shakespeare's Richard III.</prompt>
